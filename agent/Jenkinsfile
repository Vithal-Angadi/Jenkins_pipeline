pipeline{
    agent any
    stages{
        stage("Build"){
            steps{
                sh 'sleep 5'
            }
         }
        stage("Test"){
            steps{
                sh '''
                   #!/bin/bash
                   ls -lrt
                   sleep 10
                '''
            }
         }
         stage("Deploy"){
            steps{
                sh '''
                   cd /home/ubuntu
                   chmod 777 yogu
                '''
            }
         }
    }
}